{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "API completa com Typescript utilizando TDD, Clean Architecture, Design Patterns e SOLID.",
  "main": "index.js",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:verbose": "jest --passWithNoTests",
    "test:ci": "npm test -- --coverage",
    "test:integration": "npm test -- --watch -c jest-integration.config.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ederfmatos/clean-architecture-node-api-rm.git"
  },
  "keywords": [
    "clean-architecture",
    "solid",
    "tdd",
    "design patterns",
    "typescript",
    "node"
  ],
  "author": "Eder Matos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ederfmatos/clean-architecture-node-api-rm/issues"
  },
  "homepage": "https://github.com/ederfmatos/clean-architecture-node-api-rm#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "sucrase": "^3.20.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "fast-glob": "^3.2.11",
    "mongodb": "^4.4.1",
    "validator": "^13.7.0"
  }
}
